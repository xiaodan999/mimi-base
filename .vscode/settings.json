{
    "files.watcherExclude": {
        "**/routeTree.gen.ts": true
    },
    "search.exclude": {
        "**/routeTree.gen.ts": true
    },
    "files.readonlyInclude": {
        "**/routeTree.gen.ts": true
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "message": "Start to format",
                "messageAfter": "Finish formatting",
                "match": "\\.(html|css|js|ts|tsx|jsx|json)$",
                "cmd": "echo 'npx prettier ${file} --write && npx eslint --fix ${file}' && npx prettier ${file} --write && npx eslint --fix ${file}"
            }
        ]
    }
}
